{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "rootDir": ".",
    "isolatedModules": false,
    "strictNullChecks": true,
    "noEmit": true,
    "types": ["./types", "cypress"],
    "paths": {
      "@support/*": ["./support/*"]
    }
  },
  "include": ["./**/*.ts",".", "../node_modules/cypress", ]
}
